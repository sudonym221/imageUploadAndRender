<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
</head>
<body>
    <div class="container">
        <% image.forEach(function(imageS){ %>
        <div class="card" style="width: 18rem; height: 18rem;">
            <img src="/tmp/my-uploads/<%= imageS.path %> " class="card-img-top" alt="...">
            <div class="card-body">
              <p class="card-text"><%= imageS.caption %></p>
            </div>
        </div>
        <% }); %>
    </div>
</body>
</html>